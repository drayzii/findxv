<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Ibyahujwe - Ndarangisha</title>

  <!-- Custom fonts for this template-->
  <link href="../css/font-awesome.min.css" rel="stylesheet" type="text/css">


  <link href="css/ndarangisha.css" rel="stylesheet">

</head>

<body id="page-top">

  <!-- ===============================HEADER=========================================================================== -->
  <nav class="navbar navbar-expand navbar-dark bg-dark static-top">

    <a class="navbar-brand mr-1" href="ibyahujwe">Ndarangisha</a>

    <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
      <i class="fa fa-bars"></i>
    </button>

    <form class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
    </form>

    <ul class="navbar-nav ml-auto ml-md-0">


      <li class="nav-item dropdown no-arrow">
        <a class="nav-link dropdown-toggle" href="../#" id="userDropdown" role="button" data-toggle="dropdown"
          aria-haspopup="true" aria-expanded="false">
          <i class="fa fa-user-circle fa-2x fa-fw"></i>
        </a>
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
          <a class="dropdown-item" href="logout">Logout</a>
        </div>
      </li>
    </ul>

  </nav>





  <div id="wrapper">
    <!-- Sidebar -->
    <ul class="sidebar navbar-nav">
        <li class="nav-item active">
            <a class="nav-link" href="ibyahujwe">
              
                <span> Ibyahujwe </span>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="ibyatakaye">
             
                <span>Ibyatakaye</span>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="ibyatowe">
           
                <span>Ibyatowe</span>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link" href="ibyabonetse">
          
                <span>Ibyabonetse</span>
            </a>
        </li>



    </ul>



    <div class="container">

      <h3 class="text-center mt-3">Ibyahujwe</h3>
      <% data.forEach(function(element) { %>
        <div id="mycontainer" class="row my-3">
          <div class="col-lg-5 mt-2">
            <div class="card w3-card">
              <div class="card-body">
                <h5 class="card-title text-center">Indangamuntu</h5>
                <h6 class="card-subtitle mb-2 text-center"><%= element.icyatawe.amazina %></h6>
                <ul class="list-group">
                  <li class="list-group-item"><strong>Nimero Y'icyangombwa:</strong> <%= element.icyatawe.icyangombwa %></li>
                  <li class="list-group-item"><strong>Location:</strong> <%= element.icyatawe.akarere %>, <%= element.icyatawe.umurenge %>, <%= element.icyatawe.akagari %>, <%= element.icyatawe.umudugudu %></li>
                  <li class="list-group-item"><strong>Indangamuntu:</strong> <%= element.icyatawe.indangamuntu %></li>
                  <li class="list-group-item"><strong>Telefone:</strong> <%= element.icyatawe.telefone %></li>
                  <li class="list-group-item"><strong>Email:</strong> <%= element.icyatawe.email %></li>
                </ul>
              </div>
            </div>
          </div>
          <div class="col-lg-5 my-2">
            <div class="card w3-card">
              <div class="card-body">
                <h5 class="card-title text-center">Indangamuntu</h5>
                <h6 class="card-subtitle mb-2 text-muted text-center"><%= element.icyatoraguwe.amazina %></h6>
                <ul class="list-group">
                  <li class="list-group-item"><strong>Nimero Y'icyangombwa:</strong> <%= element.icyatoraguwe.icyangombwa %></li>
                  <li class="list-group-item"><strong>Location:</strong> <%= element.icyatoraguwe.akarere %>, <%= element.icyatoraguwe.umurenge %>, <%= element.icyatoraguwe.akagari %>, <%= element.icyatoraguwe.umudugudu %></li>
                  <li class="list-group-item"><strong>Indangamuntu:</strong> <%= element.icyatoraguwe.indangamuntu %></li>
                  <li class="list-group-item"><strong>Telefone:</strong> <%= element.icyatoraguwe.telefone %></li>
                  <li class="list-group-item"><strong>Email:</strong> <%= element.icyatoraguwe.email %></li>
                </ul>
              </div>
            </div>
          </div>
          <div class="col-lg-2 my-auto">
            <button class="btn btn-primary" data-toggle="modal" data-target="#viewmodal<%= element.icyatoraguwe._id %>">Proof</button>
          </div>
        </div>
      <% }); %>











    </div>


  </div>
  <!-- /#wrapper -->



  <% data.forEach(function(element) { %>
    <div class="modal fade" id="viewmodal<%= element.icyatoraguwe._id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
      aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Uwatoye</h5>
            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">Ã—</span>
            </button>
          </div>
          <div class="modal-body">

            <div>
              <div class="card w3-card">
                <div class="card-body">
                  <ul class="list-group">
                    <li class="list-group-item">
                      <img src="<%= element.icyatoraguwe.ifoto %>" class="img-fluid" alt="Responsive image">
                    </li>
                    <li class="list-group-item"><strong>Nimero Y'icyangombwa:</strong> <%= element.icyatoraguwe.icyangombwa %></li>
                    <li class="list-group-item"><strong>Ubusobanuro:</strong> <%= element.icyatoraguwe.ubusobanuro %></li>
                  </ul>
                </div>
              </div>
            </div>

          </div>
          <div class="modal-footer">
            <form action='kwemeza/<%= element.icyatoraguwe.icyangombwa %>' method="GET">
              <button class="btn btn-success" type="submit">Emeza</button>
            </form>
            <a class="btn btn-danger" data-dismiss="modal" aria-label="Close">Hakana</a>
          </div>
        </div>
      </div>
    </div>
  <% }); %>









  <!-- Bootstrap core JavaScript-->
  <script src="../js/jquery-3.3.1.min.js"></script>
  <script src="../js/bootstrap.min.js"></script>
  <script src="../js/sb-admin.min.js"></script>


</body>

</html>